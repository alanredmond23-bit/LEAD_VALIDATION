# Anthony Lead Forensics - Environment Variables Template
# Copy this file to .env and fill in your actual credentials
# IMPORTANT: Never commit .env file to git!

# Twilio API (for phone validation)
TWILIO_ACCOUNT_SID=your_twilio_account_sid_here
TWILIO_AUTH_TOKEN=your_twilio_auth_token_here

# ZeroBounce API (for email validation)
ZEROBOUNCE_API_KEY=your_zerobounce_api_key_here

# IPQualityScore API (for IP/VPN detection)
IPQS_API_KEY=your_ipqualityscore_api_key_here

# Supabase Database (for fraud history storage)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your_anon_key_here
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key_here
